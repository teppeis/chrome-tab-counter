{
  "name": "Tab counter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Counts opened tabs.",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon19.png"
  },
  "background": {
    "scripts": ["lib/background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
